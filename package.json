{
  "name": "gitlab-insights",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "concurrently --names \"next,inngest\" --prefix-colors \"cyan,magenta\" \"next dev\" \"npx inngest-cli@latest dev --no-discovery -u http://localhost:3000/api/inngest\"",
    "dev:next": "next dev",
    "dev:inngest": "npx inngest-cli@latest dev --no-discovery -u http://localhost:3000/api/inngest",
    "dev:fast-poll": "INNGEST_POLLING_CRON=\"* * * * *\" npm run dev",
    "lint": "eslint .",
    "postinstall": "prisma generate",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:seed-events": "tsx scripts/seed-events.ts"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@heroui/react": "^2.8.5",
    "@heroui/styles": "^3.0.0-beta.1",
    "@prisma/client": "^6.6.0",
    "@react-aria/table": "^3.17.8",
    "@react-stately/table": "^3.15.1",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.0.0",
    "@trpc/next": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@types/dompurify": "^3.0.5",
    "better-auth": "^1.4.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "framer-motion": "^12.23.24",
    "inngest": "^3.45.1",
    "lucide-react": "^0.556.0",
    "next": "^16.0.8",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "react": "^19.2.0",
    "react-aria-components": "^1.13.0",
    "react-dom": "^19.2.0",
    "sonner": "^2.0.7",
    "superjson": "^2.2.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.15",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.4",
    "postcss": "^8.5.3",
    "prisma": "^6.6.0",
    "tailwindcss": "^4.0.15",
    "tsx": "^4.20.6",
    "typescript": "^5.8.2",
    "vitest": "^4.0.15"
  },
  "ct3aMetadata": {
    "initVersion": "7.40.0"
  },
  "packageManager": "npm@11.4.2"
}
